<%= form_for(@project) do |f| %>

	<%= f.label :title, "Title" %>
	<%= f.text_field :title %>
	<br><br>
	<%= f.label :descript, "Description" %>
	<br>
	<%= f.text_area :descript %>
	<br>
	<br>
	<%=f.label :team_id, "Team" %>
  <%= f.collection_select(:team_id, Team.all, :id, :name) %>
  <br>
	<%= f.submit %>

<% end %>