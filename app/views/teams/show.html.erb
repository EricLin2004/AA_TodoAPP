<h1><%= @team.name %></h1>

<h3> Current Projects: </h3>
<ul>
	<% @team.projects.each do |project| %>
		<li><%= link_to project.title, project_url(project) %></li>
	<% end %>
</ul>

<h3> Current Members: </h3>
<ul>
	<% @team.users.each do |user| %>
		<li><%= link_to user.name, user_url(user) %></li>
	<% end %>
</ul>

<br>
<%= link_to "Add New Project", new_team_project_url(@team) %>
<br>
<%= link_to "Edit Team", edit_team_url(@team) %>
<br>
<%=link_to "Back to Teams", teams_url %>